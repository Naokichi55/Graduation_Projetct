<h1 class="text-4xl text-green-700 font-semibold underline">My Rake</h1>
  <% if @rackets.present? %>
    <% @rackets.each do |racket| %>
      <%= racket.product_name %>
			<%= racket.maker_name %>
			<%= racket.created_at %>
			<%= link_to '編集', '#' %>
			<%= link_to '削除', '#' %>
    <% end %>
	<% else %>
    <p> 投稿がありません </p>
  <% end %>