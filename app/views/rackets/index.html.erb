<h1 class="text-4xl text-green-700 font-semibold underline">My Rake</h1>
  <div>
    <% if @rackets.present? %>
      <% @rackets.each do |racket| %>
      <div class = "underline">
        <%= link_to racket.product_name, racket_path(racket) %> <!-- 名前を押すことで詳細画面へ遷移 -->
      </div>
        <%= racket.maker_name %>
      <div>
        <%= racket.created_at %>
			</div>
      <div>
        <%= link_to '編集', '#' %>
        <%= link_to '削除', '#' %>
      </div>
      <% end %>
	  <% else %>
      <p> 投稿がありません </p>
    <% end %>
  </div>